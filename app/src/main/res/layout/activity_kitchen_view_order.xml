<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Kitchen_ViewOrder">

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="47dp"
        android:layout_height="30dp"
        android:layout_marginStart="216dp"
        android:layout_marginTop="36dp"
        app:layout_constraintStart_toEndOf="@+id/textView30"
        app:layout_constraintTop_toBottomOf="@+id/textView29"
        app:srcCompat="@drawable/redbin" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="379dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:background="#9A9494"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Search"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="76dp"
        android:text="Order 1: "
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="293dp"
        android:layout_height="160dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:background="#9A9494"
        android:text="-Pizza:Ranch Pizza                                                                                      -Size:Personal                                                                                -Crust:Italian Crust                                                                                                -Sauce:Smoky BBQ                                                                                      -Toppings:Beef Pepperoni "
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView28" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="126dp"
        android:layout_height="46dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="Order 2: "
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView29" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="298dp"
        android:layout_height="131dp"
        android:layout_marginStart="16dp"
        android:background="#9A9494"
        android:text="-Pizza:Pepperoni                                                                                          -Size: Large                                     -Crust:Thin Crust -Sauce:Ranch Sauce                                                                                                                       -Toppings:Cherry Tomato"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView30" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="47dp"
        android:layout_height="30dp"
        android:layout_marginStart="252dp"
        android:layout_marginTop="80dp"
        app:layout_constraintStart_toEndOf="@+id/textView28"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"
        app:srcCompat="@drawable/redbin" />

    <Spinner
        android:id="@+id/spinner4"
        android:layout_width="159dp"
        android:layout_height="48dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="28dp"
        android:background="#0097A7"
        android:entries="@array/Sort_Kitchen"
        app:layout_constraintBottom_toTopOf="@+id/textView28"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/button16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="152dp"
        android:text="Home"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView20" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="72dp"
        android:layout_height="49dp"
        android:layout_marginStart="180dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView28"
        app:layout_constraintTop_toBottomOf="@+id/spinner4"
        app:layout_constraintVertical_bias="0.061"
        app:srcCompat="@drawable/request_delivery" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="68dp"
        android:layout_height="50dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toEndOf="@+id/textView30"
        app:layout_constraintTop_toBottomOf="@+id/textView29"
        app:srcCompat="@drawable/request_delivery" />

</androidx.constraintlayout.widget.ConstraintLayout>